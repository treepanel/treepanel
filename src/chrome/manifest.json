{
  "name": "Octotree", 
  "version": "1.2.0",
  "manifest_version": 2,
  "author": "Buu Nguyen",
  "description": "Display GitHub code in tree format",
  "homepage_url": "https://github.com/buunguyen/octotree",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://github.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "css": [
        "inject.css"
      ],
      "js": [
        "inject.js"
      ],
      "run_at": "document_start"
    }
  ]
}