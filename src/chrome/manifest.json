{
  "name": "Octotree",
  "version": "1.3.0",
  "manifest_version": 2,
  "author": "Buu Nguyen",
  "description": "Display GitHub code in tree format",
  "homepage_url": "https://github.com/buunguyen/octotree",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://github.com/*",
    "tabs",
    "storage"
  ],
  "optional_permissions": [
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "async.js",
      "background.js"
    ],
    "persistent": true
  },
