{
  "name": "treepanel",
  "description": "GitHub on steroids",
  "version": "3.0.10",
  "private": true,
  "author": "TreePanel Authors",
  "license": "AGPL",
  "scripts": {
    "build": "gulp",
    "dist": "gulp dist --production",
    "format": "sortpack && prettier --write \"src/**/*.js\" *.js",
    "prebuild": "npm t",
    "preversion": "npm run build",
    "test": "run-s test:lint test:unit",
    "test:lint": "eslint -c .eslintrc.json src *.js",
    "test:unit": "echo WRITE SOME TESTS"
  },
  "homepage": "https://github.com/treepanel/treepanel",
  "repository": {
    "type": "git",
    "url": "https://github.com/treepanel/treepanel"
  },
  "bugs": {
    "url": "https://github.com/treepanel/treepanel/issues"
  },
  "devDependencies": {
    "crx": "5.0.1",
    "eslint": "7.15.0",
    "eslint-plugin-zacanger": "2.2.1",
    "event-stream": "4.0.1",
    "gulp": "4.0.2",
    "gulp-autoprefixer": "7.0.1",
    "gulp-clean": "0.4.0",
    "gulp-concat": "2.6.1",
    "gulp-crx-pack": "1.0.2",
    "gulp-cssmin": "0.2.0",
    "gulp-exit": "0.0.2",
    "gulp-less": "4.0.1",
    "gulp-plumber": "1.2.1",
    "gulp-preprocess": "3.0.3",
