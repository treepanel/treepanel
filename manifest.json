{
  "name": "Octotree",
  "version": "0.1.0",
  "manifest_version": 2,
  "author": "Buu Nguyen",
  "description": "Chrome extension to display GitHub code in tree format",
  "homepage_url": "https://github.com/buunguyen/octotree",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://github.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "css": [
        "lib/css/jstree.css",
        "inject.css"
      ],
      "js": [
        "lib/js/underscore.js",
        "lib/js/jquery.js",
        "lib/js/jstree.js",
        "lib/js/base64.js",
        "lib/js/github.js",
        "inject.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "lib/images/*"
  ]
}